<!-- uploadBlog.ejs -->

<!DOCTYPE html>
<html>
<head>
  <title>Upload Song</title>
</head>
<body>
  <h1>Upload Song</h1>
  
  <form method="POST" action="/resources/songs">
    <label for="title">Title:</label>
    <input type="text" name="title" id="title" placeholder="Enter the title"><br>
    
    <label for="iframeLink">Iframe Link:</label>
    <input type="text" name="iframeLink" id="iframeLink" placeholder="Enter the iframe link"><br>
    
    <label for="rating">Rating:</label>
    <input type="number" name="rating" id="rating" min="1" max="5" placeholder="Enter the rating"><br>
    
    <button type="submit">Upload Blog</button>
  </form>
  
</body>
</html>
